{
  "plugins": [],
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_",
    "include": ["."],
    "exclude": ["node_modules"]
  },
  "sourceType": "module",
  "recurseDepth": 10,
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/api",
    "recurse": true
  }
}
